{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Flask",
			"type": "python",
			"request": "launch",
			"module": "flask",
			"env": {
				"FLASK_APP": "/app/app/wsgi.py",
				"FLASK_ENV": "development",
				"FLASK_DEBUG": "0"
			},
			"args": ["run", "--no-debugger", "--host=0.0.0.0", "--port=5002"],
			"jinja": true
		}
	]
}
